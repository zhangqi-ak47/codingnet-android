<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">

        <shape>
            <corners android:radius="0dp" />
            <gradient
                android:endColor="@color/divide_line"
                android:startColor="@color/divide_line" />
        </shape>

    </item>
    <item android:id="@android:id/progress">
        <clip>

            <shape>
                <corners android:radius="0dp" />
                <gradient
                    android:endColor="@color/font_green"
                    android:startColor="@color/font_green" />
            </shape>
        </clip>
    </item>
</layer-list>